<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="GreybeardsCasino.lua"/>
	<Frame 
		name="GBC_Frame" 
		parent="UIParent" 
		toplevel="true" 
		movable="true">
		<Scripts>
			<OnLoad>
				GBC_OnLoad(self);
			</OnLoad>
			<OnEvent>
				GBC_OnEvent(self, event, ...);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>
